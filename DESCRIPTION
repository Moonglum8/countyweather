    ggmap (>= 2.6.1),
    ggplot2 (>= 2.1.0),
    geosphere (>= 1.5.1),
    lubridate (>= 1.5.6),
    purrr (>= 0.2.1),
    raster (>= 2.5.8),
    rnoaa (>= 0.6.4),
    sp (>= 1.2.3),
    stringi (>= 1.1.1),
    tibble(>= 1.2),
    tidyr (>= 0.3.1)
RoxygenNote: 5.0.1
